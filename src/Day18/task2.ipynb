    "import sqlite3\n",
    "import pandas as pd\n",
    "\n",
    "conn=sqlite3.connect(\"internship.db\")\n",
    "\n",
    "query = \"\"\"\n",
    "SELECT i.id, i.name, i.track, i.stipend, m.mentor_name FROM interns i INNER JOIN mentors m ON i.track = m.track;\n",
    "\"\"\"\n",
    "\n",
    "df=pd.read_sql_query(query,conn)\n",
    "\n",
    "print(df)"
